{
    "name": "@varhub-games/varhub-core",
    "version": "0.1.1",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "buildWatch": "tsc --project tsconfig.json -w",
        "nodemon": "nodemon --watch ./dist/*.js --ignore ./dist/web/**/*",
        "copyWebWatch": "copy-and-watch --clean --watch ./node_modules/@varhub-games/commutator/dist/**/* ./dist/web",
        "startDemon": "concurrently \\\"npm:buildWatch\\\" \\\"npm:copyWebWatch\\\" \\\"npm:nodemon\\\" ",
        "start": "node dist/index.js"
    },
    "authors": [
        "MYXOMOPX",
        "DPOH-VAR"
    ],
    "license": "ISC",
    "description": "Core of VARHUB",
    "dependencies": {
        "@varhub-games/commutator": "^1.1.1",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.5",
        "cookie-session": "^1.4.0",
        "copy-and-watch": "^0.1.5",
        "cors": "^2.8.5",
        "crc-32": "^1.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "express-ws": "^4.0.0",
        "json-stable-stringify": "^1.0.1",
        "keygrip": "^1.1.0",
        "md5": "^2.3.0",
        "nedb": "^1.8.0",
        "nedb-promises": "^4.1.1",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "session-file-store": "^1.5.0",
        "svg-captcha-express": "^1.0.8",
        "ws": "^7.4.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/express-ws": "^3.0.0",
        "@types/nedb": "^1.8.11",
        "@types/passport": "^1.0.6",
        "@types/passport-local": "^1.0.33",
        "@types/ws": "^7.4.0",
        "concurrently": "^5.3.0",
        "husky": "^5.0.9",
        "nodemon": "^2.0.7",
        "rimraf": "^3.0.2",
        "typescript": "^4.1.5"
    }
}